{
  "expo": {
    "name": "Pop TV App",
    "slug": "poptv-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-pop.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash-pop.png",
      "resizeMode": "contain",
      "backgroundColor": "#111827"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-pop.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ormartins.poptvapp"
    },
    "web": {
      "favicon": "./assets/favicon/favicon-pop.png"
    },
    "extra": {
      "eas": {
        "projectId": "c3347070-5c2d-48de-b8e1-67ac53e5e918"
      }
    },
    "plugins": [
      "expo-video"
    ]
  }
}
